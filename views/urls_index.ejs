<% include partials/_header %>
<ul>
  <% for(let url in urls) { %>
  <li>
    <div class="url">
      <div class="original-url">URL:
        <%=  urls[url].longURL %>
      </div>
      <ul>
        <li>
          Shortened:
          <a href='/u/<%= url %>'>u/<%= url %></a>
        </li>
        <li>
          Created:
          <%= urls[url].date %>
        </li>
        <li>
          Number Of Visits:
          <%= urls[url].visitNumber %>
        </li>
      </ul>
    </div>
    <div class="url-actions">
      <form method="POST" action="/urls/<%= url %>/delete">
        <button type="submit" name="delete">Delete</button></form>
      <a href="/urls/<%= url %>">Edit </a>
    </div>


    <% }; %>
</ul>
<a href="/urls/new" alt="Add new URL">add new url</a>


<% include partials/_footer %>
